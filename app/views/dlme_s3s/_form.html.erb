<%= bootstrap_form_tag(url: exhibit_s3_harvester_path(exhibit_id: current_exhibit)) do |f| %>
              <div class="form-group">
                <%= text_field_tag :url, 'https://', class: 'form-control' %>
              </div>
              <div class="form-actions">
                <div class="primary-actions">
                  <%= cancel_link "", :back, class: 'btn btn-default' %>
                  <%= f.submit t('.add_item'), class: 'btn btn-primary' %>
                </div>
              </div>

              <% end %>
